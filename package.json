{
  "name": "file-tree",
  "version": "0.1.2",
  "description": "electron file explorer tree",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager ./ App --out ./out/ --overwrite --asar --platform linux --arch x64",
    "package-win": "electron-packager ./ App --out ./out/ --overwrite --asar --platform win32 --arch x64",
    "package-darwin": "electron-packager ./ App --out ./out/ --overwrite --asar --platform darwin --arch x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novel-plus/file-tree.git"
  },
  "author": "fifteenmania",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/novel-plus/file-tree/issues"
  },
  "homepage": "https://github.com/novel-plus/file-tree#readme",
  "devDependencies": {
    "asar": "^3.1.0",
    "electron": "^15.3.0",
    "electron-packager": "^15.4.0"
  }
}
